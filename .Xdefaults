UXTerm.termName: xterm-256color
UXTerm.ttyModes: erase ^?
UXTerm.VT100.background: black
UXTerm.VT100.foreground: white
UXTerm.VT100.faceName: Fira Code,Fira Code Retina
UXTerm.VT100.faceSize: 12
UXTerm.VT100.locale: true
UXTerm.VT100.metaSendsEscape: true
UXTerm.VT100.cutNewline: false
UXTerm.VT100.charClass: 33:48,35:48,37:48,42:48,45-47:48,64:48,95:48,126:48
UXTerm.VT100.translations: #override \n\
Ctrl Shift <Key>C: copy-selection(CLIPBOARD) \n\
Ctrl Shift <Key>V: insert-selection(CLIPBOARD) \n\
Shift <Key>Prior:string(0x1b) string("[5;3~") \n\
Shift <Key>Next:string(0x1b) string("[6;3~")
Xcursor.size: 16
